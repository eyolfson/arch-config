[init]
	defaultBranch = main
[user]
	name = Jonathan Eyolfson
	email = jon@eyolfson.com
[commit]
	gpgSign = true
[tag]
	gpgSign = true
[alias]
	lol = log --oneline --graph --decorate
	lols = log --oneline --graph --decorate --simplify-by-decoration --all
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
